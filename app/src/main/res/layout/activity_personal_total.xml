<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:clipChildren="false"
    android:orientation="vertical">

    <!--<include layout="@layout/common_title"/>-->

    <RelativeLayout
        android:id="@+id/re_myorder_userName"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_60"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_nickName"
            android:layout_centerVertical="true"
            android:textColor="@color/color_323232"
            android:textSize="@dimen/size_16"/>

        <EditText
            android:id="@+id/et_userName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@null"
            android:gravity="right"
            android:enabled="false"
            android:ellipsize="end"
            android:maxEms="10"
            android:singleLine="true"
            android:textColor="@color/other_color"
            android:textSize="@dimen/size_16"/>

    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:background="@color/bg_grey"/>
    <RelativeLayout
        android:id="@+id/re_myorder_headPortrait"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_60"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_face"
            android:layout_centerVertical="true"
            android:textColor="@color/color_323232"
            android:textSize="@dimen/size_16"/>

        <ImageView
            android:id="@+id/iv_headPortrait"
            android:layout_width="@dimen/size_40"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:gravity="right"
            android:enabled="false"
            android:src="@mipmap/ic_touxiang"
            android:textSize="@dimen/size_16"/>

    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:background="@color/bg_grey"/>
    <RelativeLayout
        android:id="@+id/re_myorder_sex"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_60"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15">

        <TextView
            android:id="@+id/tv_sex1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_sex"
            android:layout_centerVertical="true"
            android:textColor="@color/color_323232"
            android:textSize="@dimen/size_16"/>

        <TextView
            android:id="@+id/tv_sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:gravity="right"
            android:textColor="@color/other_color"
            android:textSize="@dimen/size_16"/>

    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:background="@color/bg_grey"/>

    <RelativeLayout
        android:id="@+id/re_birthday"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_60"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15">

        <TextView
            android:id="@+id/edit_ll_regTwo_add"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="@string/my_birthday"
            android:textColor="@color/color_323232"
            android:textSize="@dimen/size_16"/>

        <TextView
            android:id="@+id/edit_et_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:textColor="@color/other_color"
            android:textSize="@dimen/size_16"/>

    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:background="@color/bg_grey"/>

    <RelativeLayout
        android:id="@+id/re_age"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_60"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15">

        <TextView
            android:id="@+id/et_age1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_age"
            android:layout_centerVertical="true"
            android:textColor="@color/color_323232"
            android:textSize="@dimen/size_16"/>

        <TextView
            android:id="@+id/tv_age"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:textColor="@color/other_color"
            android:textSize="@dimen/size_16"
            />
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:background="@color/bg_grey"/>

    <RelativeLayout
        android:id="@+id/re_occupation"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_60"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15">

        <TextView
            android:id="@+id/tv_occupation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_occupation"
            android:layout_centerVertical="true"
            android:textColor="@color/color_323232"
            android:textSize="@dimen/size_16"/>

        <EditText
            android:id="@+id/et_occupation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@null"
            android:enabled="false"
            android:gravity="right"
            android:maxLength="20"
            android:textColorHint="@color/other_color"
            android:textColor="@color/other_color"
            android:textSize="@dimen/size_16"/>

    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:background="@color/bg_grey"/>

    <TextView
        android:id="@+id/tv_aboutUs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:text="@string/my_about"
        android:layout_marginTop="@dimen/size_20"
        android:textColor="@color/color_323232"
        android:textSize="@dimen/size_16"/>

    <EditText
        android:id="@+id/et_aboutUs_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_marginTop="@dimen/size_10"
        android:background="@null"
        android:enabled="false"
        android:maxLength="200"
        android:textColorHint="@color/other_color"
        android:textColor="@color/other_color"
        android:textSize="@dimen/size_16"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_1"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_15"
        android:background="@color/bg_grey"/>

</LinearLayout>